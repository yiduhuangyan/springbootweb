<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.zjh.dao.mapper.TransationMapper">

	<select id="getCustomerOrderInfo" resultType="com.cn.zjh.dao.output.CustomerOrderDaoOutput">
		select 
		a.customer_name as customerName,
		a.trade_name as tradeName,
		a.number
		from
		order_info a
	</select>

</mapper>